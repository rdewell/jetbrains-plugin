<idea-plugin>
  <!-- Unique identifier of the plugin.
      Cannot be changed between the plugin versions. -->
  <id>Stacklane</id>

  <name>Stacklane</name>

  <version>1.0.5</version>

  <vendor email="plugin@stacklane.com" url="https://stacklane.com">Stacklane, Inc.</vendor>

  <description><![CDATA[
      Stacklane is the automatically
      scalable platform for
      serverless web apps,
      smart data models,
      and rapid results.

      The Stacklane WebStorm plugin syncs project files for online testing.
    ]]></description>

  <idea-version since-build="WS-172"/> <!-- targeting webstorm only -->

 <!-- <depends>com.intellij.modules.lang</depends>-->
  <depends>com.intellij.modules.platform</depends>

  <extensions defaultExtensionNs="com.intellij">
      <configurationType implementation="io.stacklane.jetbrains.SLRunConfigType" />
      <runConfigurationProducer implementation="io.stacklane.jetbrains.SLRunConfigProducer" />
  </extensions>
</idea-plugin>